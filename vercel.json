{
  "crons": [
    {
      "path": "/api/backup",
      "schedule": "0 7 * * *"
    }
  ],
  "functions": {
    "src/app/api/upload/route.ts": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "headers": [
    {
      "source": "/api/upload",
      "headers": [
        {
          "key": "x-vercel-body-size-limit",
          "value": "0"
        }
      ]
    }
  ]
}
